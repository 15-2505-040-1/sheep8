<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cloud.sheep2.ResultActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/treasurebox_back" />

    <ImageButton
        android:id="@+id/close"
        android:layout_width="282dp"
        android:layout_height="249dp"
        android:background="@null"
        android:onClick="onOpen"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977"
        app:srcCompat="@drawable/takarabako01" />

    <ImageButton
        android:id="@+id/item"
        android:layout_width="83dp"
        android:layout_height="79dp"
        android:layout_marginTop="69dp"
        android:background="@null"
        android:onClick="onGet"
        android:scaleType="fitCenter"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/item1"
        app:layout_constraintHorizontal_bias="0.501"
        tools:layout_editor_absoluteY="77dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="112dp"
        android:layout_height="92dp"
        android:layout_marginTop="403dp"
        android:backgroundTint="#ffffffff"
        android:onClick="onGet"
        android:text="タイトルに\n戻る"
        android:textSize="18sp"
        android:visibility="invisible"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
